        h1 Найдём номера под ваши пожелания
        .findApart__dateDropdown    
            +dateDropdown("Прибытие", "Выезд")
        .findApart__dropdown
            +dropdown(true, "гости")
        +button({title: "Подобрать номер", height: "44px"})